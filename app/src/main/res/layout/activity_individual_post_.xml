<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:orientation="vertical" android:layout_width="match_parent"
    android:layout_height="match_parent">

    <android.support.constraint.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <TextView
            android:id="@+id/textView7"
            android:layout_width="187dp"
            android:layout_height="32dp"
            android:layout_marginLeft="22dp"
            android:layout_marginStart="22dp"
            android:text="Title"
            android:textSize="24dp"
            app:layout_constraintLeft_toLeftOf="parent"
            tools:layout_constraintLeft_creator="1"
            tools:layout_constraintTop_creator="1"
            android:layout_marginRight="8dp"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintTop_toTopOf="parent"
            android:layout_marginTop="8dp"
            android:layout_marginBottom="8dp"
            app:layout_constraintBottom_toTopOf="@+id/textView9" />

        <TextView
            android:id="@+id/textView8"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Provider"
            android:textSize="18dp"
            app:layout_constraintBaseline_toBaselineOf="@+id/textView7"
            android:layout_marginRight="8dp"
            app:layout_constraintRight_toRightOf="parent"
            android:layout_marginLeft="8dp"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintHorizontal_bias="0.79" />

        <TextView
            android:id="@+id/textView9"
            android:layout_width="317dp"
            android:layout_height="144dp"
            android:text="Description"
            android:textSize="20dp"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintHorizontal_bias="0.274"
            android:layout_marginLeft="5dp"
            app:layout_constraintTop_toTopOf="parent"
            android:layout_marginTop="8dp"
            app:layout_constraintBottom_toBottomOf="parent"
            android:layout_marginBottom="8dp"
            app:layout_constraintVertical_bias="0.205" />

        <TextView
            android:id="@+id/textView10"
            android:layout_width="171dp"
            android:layout_height="23dp"
            android:text="Person(s) Available"
            android:textSize="18dp"
            android:layout_marginRight="8dp"
            app:layout_constraintRight_toRightOf="parent"
            android:layout_marginLeft="8dp"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintHorizontal_bias="0.34"
            app:layout_constraintBottom_toBottomOf="parent"
            android:layout_marginBottom="73dp"
            app:layout_constraintTop_toTopOf="parent"
            android:layout_marginTop="8dp"
            app:layout_constraintVertical_bias="1.0" />

        <TextView
            android:id="@+id/textView11"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            tools:layout_editor_absoluteX="10dp"
            tools:layout_editor_absoluteY="385dp" />

        <TextView
            android:id="@+id/textView12"
            android:layout_width="35dp"
            android:layout_height="26dp"
            android:text="#"
            android:textSize="18dp"
            android:layout_marginLeft="22dp"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintBaseline_toBaselineOf="@+id/textView10"
            android:layout_marginRight="8dp"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintRight_toLeftOf="@+id/textView10" />

        <Button
            android:id="@+id/button"
            android:layout_width="122dp"
            android:layout_height="38dp"
            android:text="Request"
            android:textSize="18dp"
            android:layout_marginRight="16dp"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintBaseline_toBaselineOf="@+id/editText3"
            app:layout_constraintLeft_toRightOf="@+id/editText3"
            android:layout_marginLeft="8dp" />

        <TextView
            android:id="@+id/textView13"
            android:layout_width="61dp"
            android:layout_height="24dp"
            android:text="Time"
            android:textSize="18dp"
            android:layout_marginLeft="8dp"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
            android:layout_marginBottom="109dp"
            app:layout_constraintRight_toLeftOf="@+id/textView14"
            android:layout_marginRight="8dp"
            app:layout_constraintHorizontal_bias="0.583"
            android:layout_marginTop="8dp"
            app:layout_constraintVertical_bias="1.0"
            app:layout_constraintTop_toBottomOf="@+id/textView9" />

        <TextView
            android:id="@+id/textView14"
            android:layout_width="56dp"
            android:layout_height="23dp"
            android:text="12:00"
            android:textSize="18dp"
            app:layout_constraintBaseline_toBaselineOf="@+id/textView13"
            android:layout_marginRight="227dp"
            app:layout_constraintRight_toRightOf="parent"
            android:layout_marginLeft="8dp"
            app:layout_constraintLeft_toLeftOf="@+id/textView11"
            app:layout_constraintHorizontal_bias="1.0" />

        <TextView
            android:id="@+id/textView15"
            android:layout_width="23dp"
            android:layout_height="23dp"
            android:text="-"
            app:layout_constraintBaseline_toBaselineOf="@+id/textView14"
            app:layout_constraintRight_toLeftOf="@+id/textView16"
            android:layout_marginRight="8dp"
            app:layout_constraintLeft_toRightOf="@+id/textView14"
            android:layout_marginLeft="8dp" />

        <TextView
            android:id="@+id/textView16"
            android:layout_width="55dp"
            android:layout_height="26dp"
            android:text="1:00"
            android:textSize="18dp"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintBaseline_toBaselineOf="@+id/textView15"
            app:layout_constraintHorizontal_bias="0.58" />

        <EditText
            android:id="@+id/editText3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:ems="10"
            android:inputType="textPersonName"
            android:text="Address"
            android:layout_marginLeft="22dp"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
            android:layout_marginBottom="16dp"
            android:layout_marginRight="8dp"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintTop_toTopOf="parent"
            android:layout_marginTop="8dp"
            app:layout_constraintVertical_bias="1.0" />
    </android.support.constraint.ConstraintLayout>
</LinearLayout>